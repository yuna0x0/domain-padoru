# URL to your Umami analytics script
NEXT_PUBLIC_UMAMI_SCRIPT_URL=https://analytics.example.com/script.js

# List of sites to track with Umami analytics
# Format: domain:website-id
# Multiple sites are separated by commas
#
# domain: The exact domain name to track (e.g., example.com)
# website-id: The Umami website ID from your dashboard
NEXT_PUBLIC_UMAMI_SITES=example.com:website-id-1,blog.example.com:website-id-2,docs.example.com:website-id-3
